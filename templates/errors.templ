package templates

templ E404(path string) {
  <div>
    Unable to find <pre>{path}</pre>
  </div>
  <div>
    <a href="javascript:history.back()">Click here</a> to go back, <a href="/">or here</a> to go back to the main page
  </div>
}

templ Invalid(msg string) {
  <div>
    Invalid request, {msg}
  </div>
}
